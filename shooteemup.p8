function _init()
j={x=60,y=80,speed=4}
bullets={}
shootem=0
color=0
create_b()
create_ennemi()
end

function _update()
if(btnp(❎)) shoot()
update_bullets()
update_player()
j.x=mid(0,j.x,120)
j.y=mid(0,j.y,120)

update_b()
update_ennemi()
shootem+=1
color+=1
end


function _draw()
cls()


for e in all(ennemi) do
print(e.name,e.x,e.y,color)
end

for b in all(bullets)do
spr(3,b.x,b.y)
end

for s in all (b) do
pset(s.x,s.y,s.col)
end


spr(1,j.x,j.y)
print("shoot'em' n be positive!!!",shootem,10,11)
end
