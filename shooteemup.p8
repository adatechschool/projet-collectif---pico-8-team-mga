function create_ennemi()
local n={"STRESS","SHAME",
"FEAR","ANXIETY","HARD","SAD","MAD" }
ennemi={}
for i=1,5 do
e={y=rnd(120),
x=rnd(100),speed=0.5,name=rnd(n)
}
add(ennemi,e)
end 
end

function update_ennemi()
for i in all(ennemi) do
i.y+=i.speed
if i.y>128 then
i.y=0
i.y=rnd(0)
end


for b in all(bullets) do
if hit(i,b) then
del(bullets,b)
end
end
end
end
