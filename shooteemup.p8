function update_player()
if(btn(➡️)) j.x+=j.speed
if(btn(⬅️)) j.x-=j.speed
if(btn(⬆️)) j.y-=j.speed
if(btn(⬇️)) j.y+=j.speed




for e in all(ennemi) do
if hit(e,j) then
_init()
end


end
end

